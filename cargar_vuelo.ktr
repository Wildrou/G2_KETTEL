      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>var ultima_actualizacion= java.sql.Timestamp.valueOf(ultima_actualizacion_vuelo);

if(ultima_actualizacion_avion > ultima_actualizacion)
   ultima_actualizacion = ultima_actualizacion_avion;


if(ultima_actualizacion_aerolinea > ultima_actualizacion)
   ultima_actualizacion = ultima_actualizacion_aerolinea;


if(ultima_actualizacion_recorrido > ultima_actualizacion)
   ultima_actualizacion = ultima_actualizacion_recorrido;

var duracion_string;
var capacidad_string;


 if( duracion_minutos> 600)
     duracion_string="+10 horas";


 else if ( duracion_minutos &lt;= 600  &amp;&amp; duracion_minutos > 300 )
     duracion_string="5-10 horas";

 else if ( duracion_minutos &lt;= 300 &amp;&amp; duracion_minutos > 120)
     duracion_string="2-5 horas";

 else if ( duracion_minutos &lt;= 120  &amp;&amp; duracion_minutos >= 60)
     duracion_string="1-2 horas";

 else 
     duracion_string="menos de 1 hora";



 if( numplazas> 250)
     capacidad_string="+250";


 else if ( numplazas &lt;= 250  &amp;&amp; numplazas > 150 )
     capacidad_string="150-250";

 else if ( numplazas &lt;= 150 &amp;&amp; numplazas > 90)
     capacidad_string="90-150";

 else if ( numplazas &lt;= 90  &amp;&amp; numplazas > 60)
     capacidad_string="60-90";


 else if ( numplazas &lt;= 60 &amp;&amp; numplazas >= 20)
     capacidad_string="60-90";

 else 
     capacidad_string="menos de 20";</jsScript_script>
      </jsScript>
